<?xml version="1.0"?>
<robot name="PRISMM">

  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>

  <link name="base_link">
    <visual>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://nuice_simulations/src/axis_sim/urdf/frame.dae"/>
      </geometry>
    </visual>
  </link>

  <link name="rack">
    <visual>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <material name="black"/>
      <geometry>
        <mesh filename="package://nuice_simulations/src/axis_sim/urdf/rack.dae"/>
      </geometry>
    </visual>
  </link>

  <joint name="frame_rack_link" type="fixed">
    <parent link="base_link"/>
    <child link="rack"/>
  </joint>

</robot>
